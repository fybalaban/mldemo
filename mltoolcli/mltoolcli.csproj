        <TargetFramework>net6.0</TargetFramework>
        <LangVersion>10</LangVersion>
    </PropertyGroup>

    <ItemGroup>
      <EmbeddedResource Update="TurkishStrings.resx">
        <Generator>ResXFileCodeGenerator</Generator>
